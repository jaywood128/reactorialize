<p id="notice"><%= notice %></p>

<h1>Tutorial Modules</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tutorial_modules.each do |tutorial_module| %>
      <tr>
        <td><%= link_to 'Show', tutorial_module %></td>
        <td><%= link_to 'Edit', edit_tutorial_module_path(tutorial_module) %></td>
        <td><%= link_to 'Destroy', tutorial_module, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tutorial Module', new_tutorial_module_path %>
